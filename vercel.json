{
  "redirects": [
    {
      "source": "/",
      "destination": "/docs/build/overview",
      "permanent": true
    },
    {
      "source": "/docs",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/docs/",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/docs/learn/overview",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/docs/(.*).md",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex" },
        { "key": "Content-Type", "value": "text/markdown; charset=utf-8" }
      ]
    },
    {
      "source": "/(.*)/llms.txt",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex" },
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" }
      ]
    },
    {
      "source": "/llms.txt",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex" },
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" }
      ]
    }
  ]
}
